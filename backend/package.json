{
	"name": "novel-blockchain-access-control",
	"version": "0.0.1",
	"main": "app.js",
	"license": "GNU General Public License v3.0",
	"scripts": {
		"start": "nodemon --exec ./node_modules/.bin/fastify start -l info ./src/app.js",
		"migrate:make": "./node_modules/knex/bin/cli.js --knexfile src/db/config/knexfile.js migrate:make $1",
		"migrate:latest": "./node_modules/knex/bin/cli.js --knexfile src/db/config/knexfile.js migrate:latest",
		"seed:make": "./node_modules/knex/bin/cli.js --knexfile src/db/config/knexfile.js seed:make $1",
		"seed:run": "./node_modules/knex/bin/cli.js --knexfile src/db/config/knexfile.js seed:run",
		"fastify-test": "nodemon --exec ./node_modules/.bin/fastify start -l info ./src/app.js",
		"dev": "nodemon --exec ./node_modules/.bin/fastify start -l info -P ./src/app.js",
		"test": "hardhat --network hardhat test",
		"coverage": "hardhat coverage",
		"node": "hardhat node",
		"compile": "hardhat compile",
		"local": "hardhat run --network localhost scripts/deployUser.js"
	},
	"dependencies": {
		"@fastify/autoload": "^5.8.0",
		"@fastify/cors": "^9.0.1",
		"@fastify/swagger": "^8.14.0",
		"@fastify/swagger-ui": "^3.0.0",
		"@mapbox/node-pre-gyp": "^1.0.11",
		"@nomicfoundation/hardhat-chai-matchers": "^2.0.6",
		"@nomicfoundation/hardhat-ethers": "^3.0.5",
		"@nomicfoundation/hardhat-network-helpers": "^1.0.10",
		"@nomicfoundation/hardhat-toolbox": "^4.0.0",
		"@nomicfoundation/hardhat-verify": "^2.0.4",
		"@openzeppelin/contracts": "^5.0.2",
		"@typechain/ethers-v6": "^0.5.1",
		"@typechain/hardhat": "^9.1.0",
		"ajv": "^8.12.0",
		"argon2": "^0.40.1",
		"argon2-cli": "^0.25.1",
		"dotenv": "^16.4.4",
		"dotenv-cli": "^7.3.0",
		"fastify": "^4.26.1",
		"fastify-cli": "^6.1.1",
		"hardhat": "^2.20.1",
		"hardhat-gas-reporter": "^1.0.10",
		"knex": "^3.1.0",
		"lodash": "^4.17.21",
		"moment": "^2.30.1",
		"mysql2": "^3.9.1",
		"nodemon": "^3.0.3",
		"solidity-coverage": "^0.8.10",
		"typechain": "^8.3.2"
	}
}
